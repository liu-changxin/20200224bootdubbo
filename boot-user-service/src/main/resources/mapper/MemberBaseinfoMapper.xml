<?xml version="1.0" encoding="UTF-8" ?>  
<!DOCTYPE mapper  
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"  
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lcx.mapper.MemberBaseinfoMapper">
	
	 
		<select id="selectByPrimaryKey"  resultType="com.lcx.pojo.MemberBaseinfo">
	
	
			select * from  member_baseinfo d
					
			 	<if test="params.code!=null"> d.code ='${params.code}'</if>
			 	
			order by d.registertime desc
	</select>
</mapper>  